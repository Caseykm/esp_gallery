    "concurrently": "^3.6.0",
    "express": "^4.16.2",
    "jquery": "^1.9.1",
    "jsonwebtoken": "^8.3.0",
    "jwt-decode": "^2.2.0",
    "material-icons-react": "^1.0.0",
    "mongodb": "^3.1.1",
    "mongoose": "^5.2.4",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
